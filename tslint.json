{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-string-literal": false,
    "object-literal-sort-keys": false,
    "no-conditional-assignment": false,
    "arrow-parens": [true, "ban-single-arg-parens"]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [ "dist", "node_modules" ]
  }
}
